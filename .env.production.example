# Production Firebase Configuration
# Copy this file to .env.production and fill in your production Firebase project values
# Get these values from: Firebase Console → Project Settings → General → Your apps → Web app

VITE_FIREBASE_API_KEY=your-production-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-prod.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-prod
VITE_FIREBASE_STORAGE_BUCKET=your-prod.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Environment identifier
VITE_ENV=production
