# Staging Firebase Configuration
# Copy this file to .env.staging and fill in your staging Firebase project values
# Get these values from: Firebase Console → Project Settings → General → Your apps → Web app

VITE_FIREBASE_API_KEY=your-staging-api-key
VITE_FIREBASE_AUTH_DOMAIN=sms-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=sms-staging
VITE_FIREBASE_STORAGE_BUCKET=sms-staging.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Environment identifier
VITE_ENV=staging
